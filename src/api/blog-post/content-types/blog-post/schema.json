{
  "kind": "collectionType",
  "collectionName": "blog_posts",
  "info": {
    "singularName": "blog-post",
    "pluralName": "blog-posts",
    "displayName": "Blog Post",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "Image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "Date": {
      "type": "date",
      "required": true
    },
    "Author": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::author.author"
    },
    "Content": {
      "type": "customField",
      "options": {
        "preset": "toolbar"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    },
    "seo": {
      "type": "component",
      "repeatable": true,
      "component": "shared.seo"
    },
    "Featured": {
      "type": "boolean",
      "default": false
    }
  }
}
